		{
			"name": "Listar mascotas disponibles",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "f34b9609-cab7-44e9-a6a4-a8ba2d8083b1",
						"exec": [
							"pm.environment.set(\"status\", \"available\");"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "1105848c-55e5-4fe5-97b8-ff7dc852b8a2",
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"response must be valid and have a body\", function () {",
							"     pm.response.to.be.ok;",
							"     pm.response.to.be.withBody;",
							"     pm.response.to.be.json;",
							"     pm.response.to.not.have.jsonBody(\"error\");",
							"});",
							"",
							"pm.test(\"Selecting Id of an available dog\", function () {",
							"    var jsonData = pm.response.json();",
							"    var petId;",
							"    for(var i =0 ; i<jsonData.length ; i++ )",
							"    {",
							"        petId = jsonData[i].id;",
							"    }",
							"    console.log(\"PETID TAKEN IS: \"+petId);",
							"    pm.environment.set(\"petId\", petId);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"protocolProfileBehavior": {
				"disableBodyPruning": true
			},
			"request": {
				"auth": {
					"type": "apikey",
					"apikey": [
						{
							"key": "value",
							"value": "special-key",
							"type": "string"
						},
						{
							"key": "key",
							"value": "api_key",
							"type": "string"
						},
						{
							"key": "in",
							"value": "header",
							"type": "string"
						}
					]
				},
				"method": "GET",
				"header": [],
				"body": {
					"mode": "formdata",
					"formdata": [
						{
							"key": "status",
							"value": "available",
							"type": "text",
							"disabled": true
						}
					]
				},
				"url": {
					"raw": "{{base_url}}/pet/findByStatus?status={{status}}",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"pet",
						"findByStatus"
					],
					"query": [
						{
							"key": "status",
							"value": "{{status}}"
						}
					]
				}
			},
			"response": []
		},
